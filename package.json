  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ahtohbi4/tic-tac-toe/issues"
  },
  "homepage": "https://github.com/ahtohbi4/tic-tac-toe#readme",
  "lint-staged": {
    "*.js": "lint:js",
    "*.css": "lint:css"
  },
  "pre-commit": "lint:staged",
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^7.0.0-beta.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
